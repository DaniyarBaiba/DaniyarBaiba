int [][] array =  {{1,2},{3,4}};
        int sum=0;


        for (int i = 0; i < array.length; i++) {
            for (int j = 0; j < array[i].length; j++) {
                sum += array[i][j];
           }
        }
        System.out.println("The sum of all elements of a 2D Array = "+sum);
    }
}
